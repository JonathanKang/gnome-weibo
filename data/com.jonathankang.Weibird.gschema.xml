<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="weibird">
  <schema id="com.jonathankang.Weibird" path="/com/jonathankang/weibird/">
    <key name="access-token" type="s">
      <summary>Access Token</summary>
      <description>Access Token used to access your Weibo account.</description>
      <default>''</default>
    </key>
    <key name="expires-in" type="x">
        <summary>Expires in</summary>
        <description>Lifecycle of the access token, in seconds.</description>
        <default>0</default>
    </key>
    <key name="uid" type="s">
        <summary>UID</summary>
        <description>User ID.</description>
        <default>''</default>
    </key>
  </schema>
</schemalist>
